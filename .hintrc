{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "axe/name-role-value": "off",
    "axe/text-alternatives": "off",
    "disown-opener": "off",
    "css-prefix-order": "off",
    "typescript-config/consistent-casing": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "animation-timeline",
          "animation-range",
          "animation-timeline: scroll(root)",
          "view-timeline",
          "timeline-scope",
          "-webkit-mask",
          "scrollbar-width",
          "scrollbar-width: thin",
          "scrollbar-color",
          "animation-timeline: view()",
          "mask-image",
          "mask-repeat",
          "mask-position",
          "mask-size"
        ]
      }
    ],
    "button-type": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 18.2",
    "not safari <= 18.2",
    "not and_ff <= 132",
    "not chrome <= 109",
    "not firefox <= 133"
  ]
}